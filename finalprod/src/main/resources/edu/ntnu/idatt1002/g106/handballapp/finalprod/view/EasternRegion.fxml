<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.ntnu.idatt1002.g106.handballapp.finalprod.controller.EastRegionController">
   <children>
      <Label layoutX="227.0" layoutY="15.0" text="Eastern Region">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Button accessibleHelp="this button takes you back to button selection" accessibleText="back" layoutX="14.0" layoutY="18.0" mnemonicParsing="false" onAction="#backToRegionChoice" text="Back" />
      <Button accessibleHelp="this button will ask you if you want to log out" accessibleText="logout" layoutX="534.0" layoutY="18.0" mnemonicParsing="false" onAction="#logOutMethod" text="Logout" />
      <Button accessibleHelp="this button will take you to a new window where you can create a new tournament" accessibleText="new tournament" layoutX="480.0" layoutY="91.0" mnemonicParsing="false" onAction="#toSetUpNewTournament" text="New Tournament" />
      <TableView fx:id="tableView" accessibleHelp="this tableview hold information of the different tournaments, name, start- and end date" accessibleRoleDescription="nagigate with arrows and enterkey" accessibleText="tableview" layoutY="126.0" prefHeight="274.0" prefWidth="600.0">
         <columns>
            <TableColumn fx:id="tournamentNameColumn" prefWidth="196.0" text="Tournament Name" />
            <TableColumn fx:id="startDateColumn" prefWidth="207.0" text="Start Date" />
            <TableColumn fx:id="endDateColumn" prefWidth="196.0" text="End Date" />
         </columns>
      </TableView>
      <Button accessibleHelp="this button takes you to the selected tournament" accessibleText="go to tournament" layoutX="14.0" layoutY="91.0" mnemonicParsing="false" onAction="#toSelectedTournament" text="Go To Tournament" />
   </children>
</AnchorPane>
